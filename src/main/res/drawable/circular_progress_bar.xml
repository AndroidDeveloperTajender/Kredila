<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="0"
    android:pivotX="50%"
    android:pivotY="50%"
    android:toDegrees="0">
    <item android:id="@android:id/background">
        <shape
            android:innerRadiusRatio="4"
            android:shape="ring"
            android:useLevel="false"
            android:angle="0"
            android:type="sweep"
            android:thicknessRatio="10.0">
            <solid android:color="#f7f1f1"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="-90"
            android:toDegrees="-90">
            <shape
                android:innerRadiusRatio="4"
                android:shape="ring"
                android:angle="0"
                android:type="sweep"
                android:thicknessRatio="10.0">
                <gradient
                    android:startColor="#0091EA"
                    android:centerColor="#0091EA"
                    android:centerY=".20"
                    android:endColor="#0091EA"></gradient>
            </shape>
        </rotate>
    </item>
</layer-list>
